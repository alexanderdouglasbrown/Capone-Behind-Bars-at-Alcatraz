                </imageView>
                <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" scrollEnabled="NO" editable="NO" text="America’s most famous criminal died that day, but his legend had just begun." textAlignment="center" id="177">
